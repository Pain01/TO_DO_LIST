<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.to_do_list">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity0" android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".complete_note"
            android:parentActivityName=".Notes"></activity>
        <activity
            android:name=".to_task"
            android:parentActivityName=".Task" />
        <activity
            android:name=".add_new_notes"
            android:parentActivityName=".Notes" />
        <activity
            android:name=".add_newTask"
            android:parentActivityName=".to_task" />
        <activity
            android:name=".Notes"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Task"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>